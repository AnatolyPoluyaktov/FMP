# FMP Core Environment Configuration
DB_HOST={{ db_host }}
DB_PORT={{ db_port }}
DB_NAME={{ db_name }}
DB_USER={{ db_user }}
DB_PASSWORD={{ db_password }}
DB_SSLMODE=disable

# Server Configuration
PORT={{ app_port }}
GIN_MODE=release

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN={{ telegram_bot_token }}

# Security
JWT_SECRET={{ jwt_secret }}

# Logging
LOG_LEVEL=info
